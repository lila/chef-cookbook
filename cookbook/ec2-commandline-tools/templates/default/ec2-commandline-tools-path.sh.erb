# Set path for AWS API tools packages

export AWS_PATH=<%= @aws_install_base %>

export PATH=$PATH:<%= @ec2_install_dir %>/bin:<%= @sns_install_dir %>/bin
export AWS_SNS_HOME=<%= @sns_install_dir %>

export AWS_ACCESS_KEY_ID=<%= @aws_access_key_id %>
export AWS_SECRET_ACCESS_KEY=<%= @aws_secret_key_id %>

export AWS_CREDENTIAL_FILE=/opt/aws/sns/credentials.cnf

